<popmenu@Popup>:
    auto_dismiss: True
    title: "INFO: Back To Menu"
    title_align: "center"
    title_size: "17sp"
    title_font: "z-font.otf"
    separator_height: 2
    background: "images/z-night2.png"
    font_name: "z-font.otf"
    size_hint: .85, .3
    pos_hint: {'center_x': .5,'center_y': .5}
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        spacing: "20dp"
        Button:
            text: "Are you sure, you want to\nturn back to menu? That'll\nend the current game"
            font_name: "z-font.otf"
            line_height: 1.5
            pos_hint: {'center_x': .5,'center_y': 0}
            halign: 'center'
            valign: 'bottom'
            background_color: 0,0,0,0

        BoxLayout:
            spacing:  "2dp"
            height: self.minimum_height
            width: self.minimum_width
            orientation: "horizontal"
            Button:
                text: "Yes"
                font_name: "z-font.otf"
                pos_hint: {'center_x': .3,'center_y': .6}
                size_hint: 1,.6
                background_normal: ''
                font_name: "z-font.otf"
                background_color: 202/255, 0, 103/255, 1
                on_release:  app.root.current = "Start"; root.dismiss()
            Button:
                text: "Cancel"
                font_name: "z-font.otf"
                pos_hint: {'center_x': .3,'center_y': .6}
                size_hint: 1, .6
                background_normal: ''
                font_name: "z-font.otf"
                background_color: 202/255, 0, 103/255, 1
                on_release: root.dismiss()

