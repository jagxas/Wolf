#The main widgets start here !!!

<StartingScreen>:
    name: "Start"
    canvas.before:
        Color:
            rgba: (50/255,50/255,135/255,1)
        Rectangle:
            pos: self.pos
            size: self.size
            source: "images/z-night2.png"

    Image:
        source: "images/z-moon.png"
        size_hint: (.35, .35)
        pos_hint: {'center_x': 0.5, 'center_y':.77}
        allow_stretch: True
        keep_ratio: True
    Image:
        source: "images/z-valley.png"
        size_hint: (1, .3)
        pos_hint: {'center_x': 0.5, 'center_y':0.1}
        allow_stretch: True
        keep_ratio: False

    Button:
        text: "New Game"
        size_hint: (.6, .1)
        pos_hint: {'center_x': 0.5, 'y':.45}
        #background_normal: ''
        font_name: "z-font.otf"
        background_color: 202/255, 0, 103/255, 1
        on_release:
            if root.enoughqm() == True: app.root.current = "Number"; root.manager.TF_Val = True;root.cleansing(); root.manager.transition.direction = "left"; root.manager.SchonUsed = [0, 0, 0, 0, 0, 0, 0, 0, 0]; root.manager.Indicator = 0
            else: Factory.noroles().open()
    Button:
        text: "Enable Roles"
        size_hint: (.6, .1)
        pos_hint: {'center_x': 0.5, 'y':.35}
        #background_normal: ''
        font_name: "z-font.otf"
        background_color: 202/255, 0, 103/255, 1
        on_release: app.root.current = "enable"; root.hmm(); root.manager.transition.direction = "left"

<Enable>:
    name: "enable"
    canvas.before:
        Color:
            rgba: (50/255,50/255,135/255,1)
        Rectangle:
            pos: self.pos
            size: self.size
            source: "images/z-night2.png"
    Image:
        source: "images/z-valley.png"
        size_hint: (1, .3)
        pos_hint: {'center_x': 0.5, 'center_y':0.1}
        allow_stretch: True
        keep_ratio: False

    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        BoxLayout:
            orientation: "vertical"
            size_hint_y: None
            height: self.minimum_height
            width: self.minimum_width
            spacing: "450dp"
            padding: "40dp"


            Button: #Line1
                text:"Enable & disable\nthe roles"
                font_name: "z-font.otf"
                font_size: "20sp" #diğerleri ok bu değilse boxa çevir istersen
                height: "1dp"
                background_color: 0,0,0,0
                size_hint: 1, None
                pos_hint: {'center_x': .5, 'center_y':.6}
                halign: 'center'
                valign: 'middle'


            BoxLayout:
                orientation: "vertical"
                spacing: "20dp"
                BoxLayout: #Line2
                    orientation: "horizontal"
                    spacing: "20dp"
                    Button:
                        size_hint: 1, None
                        background_color: 0,0,0,0
                        height: "400dp"
                        on_release: Factory.infowolf().open()
                        Image:
                            source: "images/z-wolf.png"
                            height: self.parent.height
                            width: self.parent.width
                            #size_hint: (1, None)
                            center_x: self.parent.center_x - (( self.parent.center_x - self.parent.x ))
                            center_y: self.parent.center_y
                            allow_stretch: True
                            keep_ratio: True

                    Button:
                        size_hint: 1, None
                        background_color: 0,0,0,0
                        height: "400dp"
                        on_release: Factory.infowitch().open()
                        Image:
                            source: "images/z-witch.png"
                            height: self.parent.height
                            width: self.parent.width
                            #size_hint: (None, None) #cback
                            #center: self.parent.center
                            center_x: self.parent.center_x - (( self.parent.center_x - self.parent.x ))
                            center_y: self.parent.center_y
                            allow_stretch: True
                            keep_ratio: True

                BoxLayout: #Line3
                    orientation: "horizontal"
                    spacing: "20dp"
                    BoxLayout:
                        orientation: "vertical"
                        width: root.width
                        height: "400dp"
                        Label:
                            id: slab
                            text: "1"
                            font_name: "z-font.otf"
                            font_size: "22sp"
                            height: "400dp"
                        Slider:
                            id: wolff
                            min: 1
                            max: 2
                            step: 1
                            orientation: "horizontal"
                            height: "100dp"
                            size_hint: 1, None
                            on_value: root.s_up(*args)
                    Switch:
                        id: witch
                        height: "100dp"
                        size_hint: 1, None
                        disabled: False
                        active: True
                        on_active: root.update(); root.least() # root.ids.seer.active = False;

            BoxLayout:
                orientation: "vertical"
                spacing: "20dp"
                BoxLayout:#Line4
                    orientation: "horizontal"
                    spacing: "20dp"
                    Button:
                        size_hint: 1,None
                        background_color: 0,0,0,0
                        height: "400dp"
                        on_release: Factory.infoseer().open()
                        Image:
                            source: "images/z-seer.png"
                            height: self.parent.height
                            width: self.parent.width
                            #size_hint: (1, None)
                            center_x: self.parent.center_x - (( self.parent.center_x - self.parent.x ))
                            center_y: self.parent.center_y
                            allow_stretch: True
                            keep_ratio: True
                    Button:
                        size_hint: 1, None
                        background_color: 0,0,0,0
                        height: "400dp"
                        on_release: Factory.infovillager().open()
                        Image:
                            source: "images/z-villager.png"
